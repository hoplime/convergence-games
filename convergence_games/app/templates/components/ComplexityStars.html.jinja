{#def
    game_crunch: GameCrunch
#}
<div class="inline-flex h-[1rem] flex-row gap-2">
    <div class="flex flex-row gap-0">
        {% for i in range(game_crunch.number) %}
            <span class="icon-[material-symbols--circle] text-primary"></span>
        {% endfor %}
        {% for i in range(game_crunch.number, 5) %}
            <span class="icon-[material-symbols--circle] opacity-50"></span>
        {% endfor %}
    </div>
</div>
